{
  "name": "cslabs-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn run watch:react",
    "watch:react": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:style": "stylelint src/**/*.scss",
    "lint:ts": "tslint -p ./",
    "lint": "echo Linting.... && run-p lint:ts lint:style && echo Finished Linting with 0 errors",
    "watch:lint": "watch \"npm run lint --silent\" ./src",
    "analyze:vendor": "source-map-explorer build/static/js/1.*",
    "analyze:app": "source-map-explorer build/static/js/main.*",
    "check-npm": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
    "preinstall": "yarn run check-npm"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@sentry/browser": "^4.3.2",
    "@types/history": "^4.7.2",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-burger-menu": "^2.2.1",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.1",
    "axios": "^0.18.0",
    "connected-react-router": "^5.0.1",
    "history": "^4.7.2",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-app-polyfill": "^1.0.2",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-burger-menu": "^2.5.4",
    "react-dev-utils": "^6.1.1",
    "react-dom": "^16.9.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^24.0.18",
    "@types/react-redux": "^6.0.10",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "npm-run-all": "^4.1.5",
    "react-scripts": "3.1.1",
    "source-map-explorer": "^1.6.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.9.4",
    "tslint": "^5.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3",
    "watch": "^1.0.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=8.10 <11.0",
    "npm": ">=6.4.0"
  }
}
